  "chapter": "17. Electronics",
  "formulas": [
    {
      "formula_math": "I_E = I_B + I_C",
      "formula_english": "Current relation in transistor."
    },
    {
      "formula_math": "\\alpha = \\frac{I_C}{I_E}",
      "formula_english": "Current amplification factor (alpha)."
    },
    {
      "formula_math": "\\beta = \\frac{I_C}{I_B}",
      "formula_english": "Current gain (beta)."
    },
    {
      "formula_math": "\\beta = \\frac{\\alpha}{1 - \\alpha}",
      "formula_english": "Relation between alpha and beta."
    },
    {
      "formula_math": "\\alpha = \\frac{\\beta}{1 + \\beta}",
      "formula_english": "Alpha in terms of beta."
    },
    {
