      "card_math": "z · 1 = 1 · z = z",
      "card_other": ""
    },
    {
      "card_title": "Additive inverse",
      "card_math": "z + (-z) = 0",
      "card_other": ""
    },
    {
      "card_title": "Multiplicative inverse",
      "card_math": "z · (1/z) = 1",
      "card_other": "where z ≠ 0"
    },
