{
  "chapter": "Vectors",
  "content": [
    {
      "card_title": "Vector",
      "card_math": "u = (x, y)",
      "card_other": ""
    },
    {
      "card_title": "Modulus/Magnitude",
      "card_math": "|u| = √(x² + y²)",
      "card_other": ""
    },
    {
      "card_title": "Unit vector",
      "card_math": "â = a/|a|",
      "card_other": ""
    },
    {
      "card_title": "Points",
      "card_math": "P₁ = (x₁, y₁), P₂ = (x₂, y₂)",
      "card_other": ""
    },
    {
      "card_title": "Distance between points",
      "card_math": "|P₁P₂| = √((x₂-x₁)² + (y₂-y₁)²) = √((z₂-z₁)²)",
      "card_other": ""
    },
    {
      "card_title": "Point dividing a line internally and internally",
      "card_math": "(x₁,x₂) + (y₁,y₂) + (z₁,z₂)",
      "card_other": ""
    },
    {
      "card_title": "Direction cosines",
      "card_math": "cos α = x/|r|, cos β = y/|r|, cos γ = z/|r|",
      "card_other": ""
    },
    {
      "card_title": "Polar form",
      "card_math": "r(x, y, z) = (|r| cos α, |r| cos β, |r| cos γ)",
      "card_other": ""
    },
    {
      "card_title": "Direction number (Direction ratio)",
      "card_math": "x = cos α |r|, y = cos β |r|, z = cos γ |r|",
      "card_other": ""
    },
    {
      "card_title": "Angle between two vectors",
      "card_math": "cos θ = a·b/(|a||b|)",
      "card_other": ""
    },
    {
      "card_title": "Projection of a on b",
      "card_math": "a⃗·b⃗ = similarly |b|",
      "card_other": ""
    },
    {
      "card_title": "Component form of projection",
      "card_math": "comp_a⃗ b = a⃗·b⃗/|a|",
      "card_other": ""
    },
